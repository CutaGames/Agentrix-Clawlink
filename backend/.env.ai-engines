AWS_REGION=us-east-1
AWS_BEARER_TOKEN_BEDROCK=your_aws_bearer_token_here

# Google Gemini 配置 (免费额度 Flash 1.5)
GEMINI_API_KEY=your_gemini_api_key_here

# AI 引擎路由策略
