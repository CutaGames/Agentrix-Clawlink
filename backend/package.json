{
  "name": "agentrix-backend",
  "version": "7.0.0",
  "description": "Agentrix V7.0 Backend API - Unified Payment Protocol",
  "author": "Agentrix Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "bash build.sh || (rm -rf dist && npx tsc -p tsconfig.json && npm run build:verify)",
    "build:nest": "nest build",
    "build:tsc": "rm -rf dist && npx tsc -p tsconfig.json",
    "build:verify": "test -f dist/main.js || (echo '‚ùå Build failed: dist/main.js not found' && exit 1)",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start:admin": "ts-node src/main-admin.ts",
    "start:admin:dev": "nodemon --watch src --ext ts --exec ts-node src/main-admin.ts",
    "start:hq": "ts-node -r tsconfig-paths/register src/main-hq.ts",
    "start:hq:dev": "ts-node-dev --respawn --transpile-only src/main-hq.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --testPathPattern=integration",
    "test:onboarding": "ts-node -r tsconfig-paths/register src/scripts/test-onboarding-flows.ts",
    "test:connectivity": "ts-node -r tsconfig-paths/register src/scripts/test-connectivity.ts",
    "setup:team": "ts-node -r tsconfig-paths/register src/scripts/setup-agentrix-team.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/data-source.ts",
    "migration:check": "ts-node src/scripts/check-migrations.ts",
    "migration:fix": "ts-node src/scripts/fix-migrations.ts",
    "fix:payee-type-enum": "ts-node scripts/fix-payee-type-enum.ts",
    "migration:v3-only": "ts-node src/scripts/run-v3-migrations-only.ts",
    "seed:products": "ts-node src/scripts/seed-products.ts",
    "seed:marketplace": "ts-node src/scripts/seed-marketplace-products.ts",
    "seed:templates": "ts-node src/database/seeds/agent-templates.seed.ts",
    "reindex:products": "ts-node src/scripts/reindex-products.ts",
    "backup:db": "ts-node src/scripts/backup-database.ts",
    "migrate:deactivate-non-unified": "ts-node src/scripts/deactivate-non-unified-products.ts",
    "migrate:to-unified": "ts-node src/scripts/migrate-products-to-unified-standard.ts",
    "create:test-products": "ts-node src/scripts/create-test-products-direct.ts",
    "docs:server": "node scripts/docs-server-8080.js"
  },
  "dependencies": {
    "@ethereum-attestation-service/eas-sdk": "^2.9.0",
    "@google/generative-ai": "^0.24.1",
    "@langchain/community": "^1.1.5",
    "@langchain/core": "^1.1.16",
    "@langchain/google-genai": "^2.1.11",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@solana/web3.js": "^1.98.0",
    "@types/express-session": "^1.18.2",
    "@types/nodemailer": "^7.0.5",
    "@types/passport-apple": "^2.0.3",
    "@types/passport-twitter": "^1.0.40",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ethers": "^6.7.0",
    "express-session": "^1.18.2",
    "groq-sdk": "^0.37.0",
    "https-proxy-agent": "^7.0.6",
    "ioredis": "^5.3.0",
    "js-yaml": "^4.1.1",
    "langchain": "^0.2.10",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.12",
    "openai": "^6.9.1",
    "passport": "^0.6.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pdf-parse": "^2.4.5",
    "pg": "^8.11.0",
    "qrcode": "^1.5.4",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stripe": "^13.0.0",
    "twitter-api-v2": "^1.29.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "undici": "^6.22.0",
    "web3": "^4.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.3.1",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.ts",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 30000,
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
