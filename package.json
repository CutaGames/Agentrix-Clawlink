    "axios": "^1.6.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.0",
    "jest-junit": "^16.0.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@types/qrcode": "^1.5.6",
    "qrcode": "^1.5.4"
  }
}
