{
  "name": "@paymind/react",
  "version": "2.2.0",
  "description": "PayMind React SDK - React components and hooks for PayMind payment integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && npm run build:esm",
    "build:esm": "tsc --module esnext --outDir dist/esm && mv dist/esm dist/index.esm.js",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,tsx"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@paymind/sdk": "^2.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.0"
  },
  "keywords": [
    "paymind",
    "payment",
    "react",
    "sdk",
    "hooks",
    "components"
  ],
  "author": "PayMind Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/paymind/sdk-react.git"
  }
